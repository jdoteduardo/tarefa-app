name: dockercompose8654702157444148338
services:
  database:
    container_name: mysqldb
    environment:
      MYSQL_DATABASE: tarefasdb
      MYSQL_PASSWORD: mv
      MYSQL_RANDOM_ROOT_PASSWORD: "1"
      MYSQL_USER: admin
    expose:
      - "3306"
    image: mysql
    networks:
      default: null
    ports:
      - mode: ingress
        target: 3306
        published: "3307"
        protocol: tcp
    restart: always
    volumes:
      - type: volume
        source: dbmysql-volume
        target: /var/lib/mysql
        volume: {}
  tarefaapi:
    build:
      context: C:\CursoWebApi\desafio-mv\TarefaAPI
      dockerfile: TarefaAPI/Dockerfile
    container_name: tarefa.api
    depends_on:
      database:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
    image: tarefaapi
    networks:
      default: null
    ports:
      - mode: ingress
        target: 8080
        protocol: tcp
      - mode: ingress
        target: 8080
        published: "5000"
        protocol: tcp
      - mode: ingress
        target: 8081
        protocol: tcp
    restart: always
    volumes:
      - type: bind
        source: C:\Users\zeedu\AppData\Roaming/Microsoft/UserSecrets
        target: /home/app/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\zeedu\AppData\Roaming/ASP.NET/Https
        target: /home/app/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
networks:
  default:
    name: dockercompose8654702157444148338_default
volumes:
  dbmysql-volume:
    name: dockercompose8654702157444148338_dbmysql-volume